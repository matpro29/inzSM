{% extends 'base.html.twig' %}

{% block title %}Informacje o ocenie{% endblock %}

{% block body %}

    <div class="container">
        <h1>{% trans %}Informacje o ocenie{% endtrans %}</h1>
        <div class="listtable"><div class="row"><div class="col-md-3 pad bold">{% trans %}Id{% endtrans %}</div><div class="col-md-9 pad"> {{ grade.id }}</div></div></div>

        <div class="listtable"><div class="row"><div class="col-md-3 pad bold">{% trans %}Ocena{% endtrans %}</div><div class="col-md-9 pad"> {{ grade.grade }}</div></div></div>
        </br>
        {{ include('table/grade/form/_delete_form.html.twig') }}
        </br>
        <a href="{{ path('grade_edit', {'id': grade.id}) }}" class="delete btn">{% trans %}Edytuj{% endtrans %}</a>
        </br> </br>
        <a href="{{ path('grade_index') }}" class="back">{% trans %}Wróć{% endtrans %}</a>
    </div>
{% endblock %}
